{
  "manifest_version": 3,
  "name": "网页信息提取器",
  "version": "1.0",
  "description": "提取网页的全部信息，包括HTML、文本、图片、链接和元数据",
  "permissions": ["activeTab", "scripting", "downloads"],
  "action": {
    "default_popup": "popup.html",
    "default_title": "提取网页信息"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}